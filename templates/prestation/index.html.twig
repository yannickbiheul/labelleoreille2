{% extends 'base.html.twig' %}

{% block title %}La Belle Oreille | Prestations{% endblock %}

{% block body %}
<section id="prestations">
    <h1>Prestations</h1>
    <div class="row">
        {% for prestation in prestations %}
            <div class="col-md-4">
                <h2>{{ prestation.titre }}</h2>
                {# <p>{{ prestation.categorie }}</p> #}
                <p>{{ prestation.contenu1 }}</p>
                <p>{{ prestation.contenu2 }}</p>
            </div>
        {% endfor %}
    </div>
</section>
{% endblock %}
